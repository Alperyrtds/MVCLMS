</head>

<body id="page-top">
   
    <nav class="navbar navbar-expand-lg bg-secondary text-uppercase fixed-top" id="mainNav">
        <div class="container">
            <a class="navbar-brand js-scroll-trigger" href="#page-top">Kütüphane Sistemi</a>
            <button class="navbar-toggler navbar-toggler-right text-uppercase font-weight-bold bg-primary text-white rounded" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                Menu
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item mx-0 mx-lg-1">
                        <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#portfolio">Kitaplarımız</a>
                    </li>
                    <li class="nav-item mx-0 mx-lg-1">
                        <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#about">Hakkımızda</a>
                    </li>
                    <li class="nav-item mx-0 mx-lg-1">
                        <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#contact">İletişim</a>
                    </li>
                    <li class="nav-item mx-0 mx-lg-1">
                        <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="/Login/SignIn">Giriş Yap</a>
                    </li>
                    <li class="nav-item mx-0 mx-lg-1">
                        <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="/Register/SignUp">Üye Ol</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    
    <header class="masthead bg-primary text-white text-center">
        <div class="container d-flex align-items-center flex-column">
            
            <img class="masthead-avatar mb-5" src="~/Web/img/avataaars.svg" alt="">
            
            <h1 class="masthead-heading text-uppercase mb-0">Kütüphanemize Hoşgeldiniz...</h1>
            
            <div class="divider-custom divider-light">
                <div class="divider-custom-line"></div>
                <div class="divider-custom-icon">
                    <i class="fas fa-star"></i>
                </div>
                <div class="divider-custom-line"></div>
            </div>
            
            <p class="masthead-subheading font-weight-light mb-0">
                "Kesinlikle bilmeniz gereken tek şey, kütüphanenin nerede olduğudur."

                - Albert Einstein
            </p>

        </div>
    </header>
   
    <section class="page-section portfolio" id="portfolio">
        <div class="container">
           
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Kitaplarımız</h2>
            
            <div class="divider-custom">
                <div class="divider-custom-line"></div>
                <div class="divider-custom-icon">
                    <i class="fas fa-star"></i>
                </div>
                <div class="divider-custom-line"></div>
            </div>
            
            <div class="row">
                
                @foreach (var item in Model.Value1)
                {
                    <div class="col-md-6 col-lg-4">
                        <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#portfolioModal1">
                            <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                                <div class="portfolio-item-caption-content text-center text-white">
                                    <i class="fas fa-plus fa-3x"></i>
                                </div>
                            </div>
                            <img class="img-fluid" src="@item.BookPreview" alt="">
                        </div>
                    </div>
                }



            </div>
        </div>
    </section>
    
    <section class="page-section bg-primary text-white mb-0" id="about">
        <div class="container">
            
            <h2 class="page-section-heading text-center text-uppercase text-white">Hakkımızda</h2>
            
            <div class="divider-custom divider-light">
                <div class="divider-custom-line"></div>
                <div class="divider-custom-icon">
                    <i class="fas fa-star"></i>
                </div>
                <div class="divider-custom-line"></div>
            </div>
            
            <div @*class="row-cols-2"*@ @*style="margin-left:120px"*@>
                <div @*class="col-lg-2 ml-auto*@">
                    @foreach (var item in Model.Value2)
                    {
                        @item.Description
                    }
                </div>

            </div>
            
        </div>
    </section>
    
    <section class="page-section" id="contact">
        <div class="container">
            
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">İletişim</h2>
            
            <div class="divider-custom">
                <div class="divider-custom-line"></div>
                <div class="divider-custom-icon">
                    <i class="fas fa-star"></i>
                </div>
                <div class="divider-custom-line"></div>
            </div>
            
            <div class="row">
                <div class="col-lg-8 mx-auto">
                   
                    @using (Html.BeginForm("Index", "ShowCase", FormMethod.Post))
                    {
                        <form name="sentMessage" id="contactForm" novalidate="novalidate">
                            <div class="control-group">
                                <div class="form-group floating-label-form-group controls mb-0 pb-2">
                                    <label>İsim</label>
                                    <input class="form-control" name="Name" id="name" type="text" placeholder="İsminiz" required="required" data-validation-required-message="Lütfen adınızı giriniz.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="control-group">
                                <div class="form-group floating-label-form-group controls mb-0 pb-2">
                                    <label>Email Adresi</label>
                                    <input class="form-control" name="Email" type="email" placeholder="Email Adresiniz" required="required" data-validation-required-message="Lütfen e-posta adresinizi giriniz.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="control-group">
                                <div class="form-group floating-label-form-group controls mb-0 pb-2">
                                    <label>Mesaj Konusu</label>
                                    <input class="form-control" name="Subject" id="phone" type="tel" placeholder="Mesajınızın Konusu" required="required" data-validation-required-message="Lütfen Mesajınızın Konusunu giriniz.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="control-group">
                                <div class="form-group floating-label-form-group controls mb-0 pb-2">
                                    <label>Mesajınız</label>
                                    <textarea class="form-control" name="Message" id="message" rows="5" placeholder="Mesajınız" required="required" data-validation-required-message="Lütfen bir mesaj giriniz."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <br>
                            <div id="success"></div>
                            <div class="form-group">
                                <button type="submit" class="btn btn-primary btn-xl" id="sendMessageButton">Gönder</button>
                            </div>
                        </form>
                    }
                </div>
            </div>

        </div>
    </section>
    
